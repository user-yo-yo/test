package jp.co.internous.action;

public class Tekitou {
	public static void main(String args[]) {
        msg("佐藤");
        msg("山田", "はじめまして");
    }

    static void msg(String name) {
        System.out.println("こんにちは、" + name + "です");
    }

    static void msg(String name2) {
        System.out.println("、" + name2 + "です");
    }
}