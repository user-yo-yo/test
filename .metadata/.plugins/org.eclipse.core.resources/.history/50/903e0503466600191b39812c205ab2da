package main;

public class Na{
	public static int total = 0;
	public static int a[][] = new int [][] { { 1, 2 }, { 2, 3, 4, } };
	public static int q;
	public static int w;

	public static void main(String[] args){
		System.out.println("Aの場合");
		sub(0,0);
		System.out.println("Bの場合");
		sub(0,0);
		System.out.println("Cの場合");
		sub(0,0);
		System.out.println("Dの場合");
		sub(0,0);
	}


	private static void sub(int q,int w){
		for (int i = 0; i < a.length; i++) {
			for (int j = q; j < a[w].length; j++) {
				total += a[i][j];
			}
		}
	}
}
