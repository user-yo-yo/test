		String result = SUCCESS;

	if(!(userID.equals(""))
		&&!(password.equals(""))
		&&!(familyName.equals(""))
		&&!(firstName.equals(""))
		&&!(familyNameKana.equals(""))
		&&!(firstNameKana.equals(""))
		&&!(email.equals("")))
		{
		session.put("userId",userId);
		session.put("password",password);
