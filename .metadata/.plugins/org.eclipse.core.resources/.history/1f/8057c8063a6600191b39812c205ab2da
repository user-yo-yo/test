package main;

public class Na{
	public static void main(String[] args){
		int a[][] = new int [][] { { 1, 2 }, { 2, 3, 4, } };
		int total = 0;


			for (int i = 0; i < a.length; i++) {
				for (int j = 0; j < a[i].length; j++) {
					total += a[i][j];
				}
			}


